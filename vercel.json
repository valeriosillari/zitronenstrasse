{
  "name": "zitronenstrasse",
  "version": 2,
  "alias": ["zitronenstrasse.com", "www.zitronenstrasse.com"],
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {}
    }
  ],
  "regions": ["bru", "cdg1", "arn1"],
  "build": {
    "env": {
      "GOOGLE_MAP_API_KEY": "@zitronenstrasse_google_map_api_key",
      "GOOGLE_ANALYTICS_KEY": "@zitronenstrasse_google_analytics_key"
    }
  }
}
